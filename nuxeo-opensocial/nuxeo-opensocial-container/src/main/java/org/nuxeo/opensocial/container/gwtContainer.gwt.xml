<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='gwtContainer'>
	<inherits name='com.google.gwt.i18n.I18N' />
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<inherits name="com.google.gwt.inject.Inject" />
	<inherits name="com.google.gwt.resources.Resources" />

	<inherits name='com.allen_sauer.gwt.dnd.gwt-dnd' />
	<inherits name='org.nuxeo.opensocial.layout' />
	<inherits name='org.nuxeo.opensocial.wysiwyg.RichTextEditor' />
	<inherits name='net.customware.gwt.presenter.Presenter' />
	<inherits name='net.customware.gwt.dispatch.Dispatch' />
	<inherits name="com.google.gwt.json.JSON" />
	<inherits name='org.restlet.GWT' />

	<extend-property name='locale' values='en' />
  <extend-property name='locale' values='fr' />

  <extend-property name='locale' values='ar' />
  <extend-property name='locale' values='ca' />
  <extend-property name='locale' values='cn' />
  <extend-property name='locale' values='de' />
  <extend-property name='locale' values='es' />
  <extend-property name='locale' values='eu' />
  <extend-property name='locale' values='gl' />
  <extend-property name='locale' values='it' />
  <extend-property name='locale' values='ja' />
  <extend-property name='locale' values='pl' />
  <extend-property name='locale' values='pt' />
  <extend-property name='locale' values='pt_BR' />
  <extend-property name='locale' values='ru' />
  <extend-property name='locale' values='vn' />

	<set-property name="user.agent" value="gecko1_8"></set-property>

	<entry-point class='org.nuxeo.opensocial.container.client.Container' />

	<source path='client' />
	<source path='shared' />
</module>
