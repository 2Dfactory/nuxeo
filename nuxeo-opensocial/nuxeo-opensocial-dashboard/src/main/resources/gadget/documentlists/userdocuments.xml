<?xml version="1.0" encoding="UTF-8"?>
<Module>
  <ModulePrefs title="All accessible documents" category="business" render_inline="required" scrolling="false" author="Ian Smith" directory_title="Enhanced Bookmarks" author_aboutme="nuxeo" singleton="false">
      <Require feature="opensocial-0.8" />
      <Require feature="dynamic-height" />
      <Require feature="setprefs" />
      <Require feature="settitle" />
  </ModulePrefs>
  <UserPref name="title" display_name="Title" required="false" default_value="Documents"/>

  <Content type="html">
    <![CDATA[
    <script>
       var QM_Name="USER_DOCUMENTS";
       var testMode=false;
    </script>
    <script type="text/javascript" language="javascript" src="base64.js"></script>
    <script type="text/javascript" language="javascript" src="documentlists.js"></script>
    <link rel="stylesheet" type="text/css" href="documentlists.css"/>
    <div id="nxDocumentList">
        <div class="pageNavigationControls">
        <input type="image" src="/nuxeo/icons/action_page_rewind.gif" onclick="firstPage()"/>
        <input type="image" src="/nuxeo/icons/action_page_previous.gif" onclick="prevPage()"/>
        <span class="currentPageStatus" id="nxDocumentListPage"></span>
        <input type="image" src="/nuxeo/icons/action_page_next.gif" onclick="nextPage()"/>
        <input type="image" src="/nuxeo/icons/action_page_fastforward.gif"onclick="lastPage()"/>
        </div>
        <table id="" class="dataList">
            <thead>
                <tr>
                    <th/>
                    <th>Document</th>
                    <th/>
                    <th>Modified</th>
                    <th>Author</th>
                    <th/>
                </tr>
            </thead>
            <tbody id="nxDocumentListData">
                <tr><td><img src="/nuxeo/waitdialog/NXwaiter.gif"></td></tr>
            </tbody>
        </table>
    </div>
     ]]>
  </Content>
</Module>