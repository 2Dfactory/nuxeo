<component name="org.nuxeo.opensocial.key.contrib">

  <!-- This is the key used for symetric encryption of the security token (st)
  used when calling the ifr servlet.  -->
  <extension target="org.nuxeo.opensocial.service" point="cryptoConfig">
    <key container="default">
    <!--created with this command line on unix 
    dd if=/dev/random bs=32 count=1  | openssl base64 > /tmp/key.txt
    this must MATCH EXACTLY the configuration in opensocial-config-default-contrib.xml
    -->
      <value>Wggivegg863gcsf6QHiPEIz4VjZZOwY6g5+kg/ktqTE=</value>
    </key>
  </extension>

</component>
